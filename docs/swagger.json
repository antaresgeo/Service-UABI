  "openapi": "3.0.0",
  "info": {
    "title": "SABI | Sistema para la Administración de Bienes Inmuebles",
    "version": "0.3.0",
    "description": "Documentación para el servicio Core de SABI | Sistema para la Administración de Bienes Inmuebles"
